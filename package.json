{
  "name": "v-shared-element",
  "version": "1.0.3",
  "description": "Declarative shared-element transitions for Vue.js",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/iife/index.js",
  "types": "dist/types/",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "build": "rollup --config",
    "dev": "npm run build -- --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justintaddei/v-shared-element.git"
  },
  "keywords": [
    "vue",
    "directive",
    "page transition",
    "shared",
    "element",
    "transition",
    "nuxt"
  ],
  "author": "Justin Taddei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justintaddei/v-shared-element/issues"
  },
  "homepage": "https://github.com/justintaddei/v-shared-element#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "prettier": "^2.0.5",
    "rollup": "^2.7.6",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "jest": "^25.3.0",
    "@types/jest": "^25.2.1",
    "ts-jest": "^25.4.0"
  },
  "dependencies": {
    "illusory": "^1.2.0"
  }
}
